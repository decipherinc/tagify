  "keywords": [
    "tagify",
    "jquery",
    "tag",
    "tags"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "qunit": "~1.12.0",
    "jquery": "~2.0.3",
    "sinon-qunit": "~1.0.0",
    "sinonjs": "~1.7.1"
  }
}
